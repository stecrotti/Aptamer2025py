#!/bin/bash

declare -a pseudo_counts=(0.95000000 0.96837722 0.99400516 0.99886354 0.99978456 0.99995916 0.99999226 0.99999853 0.99999972 0.99999995 0.99999999)

declare -a ns=(0 1 2)

for i in ${!pseudo_counts[@]}; do
    for n in ${!ns[@]}; do
        pixi run python all_datasets_pseudocount.py ${n} ${pseudo_counts[$i]} &
    done
    wait
done

wait