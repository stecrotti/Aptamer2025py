#!/bin/bash

declare -a pseudo_counts=(0.00000000 0.00000001 0.00000005 0.00000028 0.00000147 0.00000774 0.00004084 0.00021544 0.00113646 0.00599484 0.03162278 0.05000000 0.10000000 0.15000000 0.20000000 0.25000000 0.30000000 0.35000000 0.40000000 0.45000000 0.50000000 0.55000000 0.60000000 0.65000000 0.70000000 0.75000000 0.80000000 0.85000000 0.90000000 0.95000000 0.96837722 0.99400516 0.99886354 0.99978456 0.99995916 0.99999226 0.99999853 0.99999972 0.99999995 0.99999999)

declare -a ns=(0 1 2)

for i in ${!pseudo_counts[@]}; do
    for n in ${!ns[@]}; do
        pixi run python all_datasets_pseudocount.py ${n} ${pseudo_counts[$i]} &
    done
    wait
done

wait